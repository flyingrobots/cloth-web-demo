{
  "entryPoints": ["src/engine", "src/lib"],
  "entryPointStrategy": "expand",
  "plugin": ["typedoc-plugin-markdown"],
  "out": "docs/api",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "disableSources": true,
  "includeVersion": true,
  "gitRevision": "HEAD",
  "categorizeByGroup": false,
  "readme": "docs/api/README.md"
}
